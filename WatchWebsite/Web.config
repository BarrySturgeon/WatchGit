   <system.webServer>
    <defaultDocument>
      <files>
        <add value="~/Pages/Index.aspx" />
      </files>
    </defaultDocument>
  </system.webServer>

  <system.web>
    <trust level="Full" />
    <compilation debug="true" targetFramework="4.6.1">
      <assemblies>
        <!-- what the fuck? we need SSL! -->
     <!--<add assembly="System.Security, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />-->   
